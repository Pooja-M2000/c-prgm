#include <stdio.h>
void main ()
 {
   int a, b; 
   printf("Enter the number of coordinate \n");
   scanf("%d%d",&a, &b); 
   if(a>0&&b>0)
     printf("\n First quadrant ");
   else if(a<0&&b>0)
     printf("\n second quadrant ");
   else if(a<0&&b<0)
     printf("\nThird quadrant ");
   else if (a>0&&b<0)
     printf("\n Forth quadrant ");
   else 
     printf("origin");
  
}
